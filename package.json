{
  "name": "all-mangas-reader",
  "version": "2.0.5",
  "description": "All Mangas Reader",
  "author": "Pierre-Louis Duhoux <pl.duhoux@gmail.com>",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js --progress --hide-modules",
    "build:dev": "cross-env NODE_ENV=development webpack --config webpack.config.js --progress --hide-modules",
    "build:dev-watch": "cross-env NODE_ENV=development webpack --config webpack.config.js --progress --hide-modules --hot --watch",
    "build-zip": "node scripts/build-zip.js",
    "manifest:specify": "node scripts/update-manifest.js",
    "watch": "npm run build -- --watch",
    "watch:dev": "npm run build:dev -- --watch"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "jquery": "^3.3.1",
    "jquery-modal": "^0.9.1",
    "jquery.scrollto": "^2.1.2",
    "vue": "^2.5.17",
    "vue-gallery": "^1.4.0",
    "vuetify": "^1.2.1",
    "vuex": "^3.0.1",
    "vuex-shared-mutations": "0.0.3",
    "webextension-polyfill": "^0.2.1"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.0.5",
    "css-loader": "^1.0.0",
    "file-loader": "^0.11.2",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.23.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^1.1.1",
    "vue-loader": "^15.4.1",
    "vue-template-compiler": "^2.5.17",
    "vue-style-loader": "^4.1.2",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-chrome-extension-reloader": "^0.8.3",
    "webpack-shell-plugin": "^0.5.0",
    "zip-dir": "^1.0.2"
  }
}
