{
  "name": "all-mangas-reader",
  "version": "2.2.14",
  "description": "All Mangas Reader",
  "author": "Pierre-Louis Duhoux <pl.duhoux@gmail.com>",
  "scripts": {
    "build": "cd ./src/mirrors && node ./update-ws.js && cd ../.. && cross-env NODE_ENV=production webpack --config webpack.config.js --progress --hide-modules",
    "build:dev": "cd ./src/mirrors && node ./update-ws.js && cd ../.. &&  cross-env NODE_ENV=development webpack --config webpack.config.js --progress --hide-modules",
    "build-zip": "node scripts/build-zip.js",
    "manifest:specify": "node scripts/update-manifest.js",
    "watch": "yarn run build --watch",
    "watch:dev": "yarn run build:dev --watch",
    "watch:dev:hot": "yarn run build:dev --watch --hot",
    "watch:dev:chrome": "yarn run watch:dev --chrome",
    "watch:dev:firefox": "yarn run watch:dev --firefox",
    "android:dev": "yarn run build:dev && yarn run manifest:specify -firefox && yarn run build-zip -xpi -push",
    "discord:beta": "node scripts/discord-notify.js"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "dompurify": "^2.2.6",
    "ejs": "^3.1.5",
    "jquery": "^3.5.1",
    "vue": "^2.6.12",
    "vue-gallery": "^1.5.0",
    "vue-scrollto": "^2.20.0",
    "vuetify": "^1.5.24",
    "vuex": "^3.6.0",
    "vuex-shared-mutations": "0.0.7",
    "webextension-polyfill": "^0.7.0"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^7.0.3",
    "css-loader": "^1.0.1",
    "node-sass": "^4.14.1",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^1.1.2",
    "vue-loader": "^15.9.6",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.12",
    "web-ext": "^4.3.0",
    "webpack": "^4.44.2",
    "webpack-chrome-extension-reloader": "^0.8.3",
    "webpack-cli": "^3.3.12",
    "webpack-shell-plugin": "^0.5.0",
    "zip-dir": "^1.0.2"
  }
}
